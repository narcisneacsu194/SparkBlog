{{#partial "content"}}
    <section>
        <div class="container">
            <div class="entry-list">

            {{#each entries}}
                <article>
                    <h2><a href="/detail/{{slug}}">{{title}}</a></h2>
                    <time datetime="{{date}}">{{date}}</time>
                </article>
            {{/each}}

                </div>
            </div>
        </section>
{{/partial}}

{{> base.hbs}}